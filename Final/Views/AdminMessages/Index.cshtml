@model IEnumerable<Final.Models.AdminMessage>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminDashBaord.cshtml";
}

<h2>Inbox</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Message_desc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeStemp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Message_desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeStemp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.UserName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
